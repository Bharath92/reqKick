{
  "name": "shippable.reqKick",
  "license": "MIT",
  "description": "Shippable build request kick-starter",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shippable/reqKick.git"
  },
  "dependencies": {
    "async": "2.5.0",
    "underscore": "1.8.3",
    "winston": "2.4.0"
  },
  "jshintConfig": {
    "node": true,
    "undef": true,
    "noarg": true,
    "white": true,
    "esnext": true,
    "strict": true,
    "newcap": true,
    "nonbsp": true,
    "eqeqeq": true,
    "unused": true,
    "maxlen": 80,
    "indent": 2,
    "bitwise": true,
    "noempty": true,
    "latedef": "nofunc",
    "maxdepth": 5,
    "quotmark": "single",
    "trailing": true,
    "maxparams": 20,
    "globalstrict": true,
    "maxcomplexity": 20,
    "maxstatements": 50,
    "predef": [
      "logger",
      "util",
      "async",
      "_"
    ]
  }
}
